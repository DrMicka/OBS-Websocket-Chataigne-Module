{
   "name":"OBS Websocket",
   "type":"WebSocket Client",
   "path":"Software",
   "version":"2.0.0",
   "description":"OBS Websocket Module",
   "url":"https://github.com/Edrig/OBS-Websocket-Chataigne-Module",
   "downloadURL":"https://github.com/Edrig/OBS-Websocket-Chataigne-Module/archive/master.zip",
   "hasInput":true,
   "hasOutput":true,
   "hideDefaultCommands":true,

   "parameters":{
      "eventSubscriptions": { "type":"Integer" },
      "password": { "type":"String" }
   },
	"defaults":
	{		
		"Protocol":"Lines",
		"autoAdd":false,
		"serverPath":"127.0.0.1:4455"
	},

   "scripts":[
      "OBS.js"
   ],
   "commands":{
   "Raw Command":{
         "menu":"",
         "callback":"sendObsRawCommand",
         "parameters":{
            "data":{
               "type":"String"
            }
         }
      },

   "Play Media":{
         "menu":"Media",
         "callback":"OBSPlayMedia",
         "parameters":{
            "inputName":{
               "type":"String"
            }
         }
      },

   "Pause Media":{
         "menu":"Media",
         "callback":"OBSPauseMedia",
         "parameters":{
            "inputName":{
               "type":"String"
            }
         }
      },

   "Restart Media":{
         "menu":"Media",
         "callback":"OBSRestartMedia",
         "parameters":{
            "inputName":{
               "type":"String"
            }
         }
      },

   "Stop Media":{
         "menu":"Media",
         "callback":"OBSStopMedia",
         "parameters":{
            "inputName":{
               "type":"String"
            }
         }
      },    
      
   "Next Media":{
         "menu":"Media",
         "callback":"OBSNextMedia",
         "parameters":{
            "inputName":{
               "type":"String"
            }
         }
      },    
      
   "Previous Media":{
         "menu":"Media",
         "callback":"OBSPreviousMedia",
         "parameters":{
            "inputName":{
               "type":"String"
            }
         }
      },    
      
   "Set Media Time":{
         "menu":"Media",
         "callback":"OBSSetMediaTime",
         "parameters":{
            "sourceName":{
               "type":"String"
            },
            "timestamp":{
               "type":"Integer"
            }
         }
      },    
      
   "ScrubMedia":{
         "menu":"Media",
         "callback":"OBSScrubMedia",
         "parameters":{
            "sourceName":{
               "type":"String"
            },
            "timeOffset":{
               "type":"Integer"
            }
         }
      },    
      
   "Set Volume":{
         "menu":"Volume",
         "callback":"OBSSetVolume",
         "parameters":{
            "inputName":{
               "type":"String"
            },
            "volume":{
               "type":"Float"
            }
         }
      },    
      
   "Set Volume Db":{
         "menu":"Volume",
         "callback":"OBSSetVolumeDb",
         "parameters":{
            "inputName":{
               "type":"String"
            },
            "volume":{
               "type":"Float"
            }
         }
      },    
      
   "Set Mute":{
         "menu":"Volume",
         "callback":"OBSSetMute",
         "parameters":{
            "inputName":{
               "type":"String"
            },
			"inputMuted":{
               "type":"String"
            },
         }
      },    
      
   "Toggle Mute":{
         "menu":"Volume",
         "callback":"OBSToggleMute",
         "parameters":{
            "inputName":{
               "type":"String"
            }
         }
      },    
  
   "TakeSourceScreenshot":{
         "menu":"Source",
         "callback":"OBSTakeSourceScreenshot",
         "parameters":{
            "sourceName":{
               "type":"String"
            },
			"imageFormat":{
               "type":"String"
            },
			"imageFilePath":{
               "type":"String"
            },
         }
      },    
      
   "RefreshBrowserSource":{
         "menu":"",
         "callback":"OBSRefreshBrowserSource",
         "parameters":{
            "sourceName":{
               "type":"String"
            }
         }
      },    
      
   "SetCurrentProfile":{
         "menu":"Profile",
         "callback":"OBSSetCurrentProfile",
         "parameters":{
            "profileName":{
               "type":"String"
            }
         }
      },    
      
   "Start Recording":{
         "menu":"Recording",
         "callback":"OBSStartRecording",
         }
      },    
      
   "Stop Recording":{
         "menu":"Recording",
         "callback":"OBSStopRecording",
         }
      },    
      
   "Toggle Pause Recording":{
         "menu":"Recording",
         "callback":"OBSPauseRecording",
         }
      },    
      
   "Toggle Resume Recording":{
         "menu":"Recording",
         "callback":"OBSToggleResumeRecording",
         }
      },    
      
   "SetCurrentSceneCollection":{
         "menu":"Scene",
         "callback":"OBSSetCurrentSceneCollection",
         "parameters":{
            "sceneCollectionName":{
               "type":"String"
            }
         }
      },    
      
   "SetCurrentProgramScene":{
         "menu":"Scene",
         "callback":"OBSSetCurrentScene",
         "parameters":{
            "sceneName":{
               "type":"String"
            }
         }
      },    
      
   "SetSceneTransitionOverride":{
         "menu":"Transition",
         "callback":"OBSSetSceneTransitionOverride",
         "parameters":{
            "sceneName":{
               "type":"String"
            },
			"transitionName":{
               "type":"String"
            },
			"transitionDuration":{
               "type":"String"
            },
         }
      },    
      
   "SetPreviewScene":{
         "menu":"Scene",
         "callback":"OBSSetPreviewScene",
         "parameters":{
            "sceneName":{
               "type":"String"
            }
         }
      },    
      
   "SetCurrentTransition":{
         "menu":"Transition",
         "callback":"OBSSetCurrentTransition",
         "parameters":{
            "transitionName":{
               "type":"String"
            }
         }
      },    
      
   "SetTransitionDuration":{
         "menu":"Transition",
         "callback":"OBSSetTransitionDuration",
         "parameters":{
            "transitionDuration":{
               "type":"String"
            }
         }
      }
   }
}
